<script>
var careproLib = careproLib
  careproLib.init().then(function(res){
      console.log(res)
      if(res.user){
          console.log("res.user -> ok")
          document.querySelector('header').classList.add('login');
      }else{
          console.log("res.user -> no")
      }
  })
  var logoutButton = document.querySelector('#logout')
  logoutButton.addEventListener('click',function(){
      careproLib.logout().then(function(){
          //- logout後の処理
          console.log("logout")
          document.querySelector('header').classList.remove('login')
          document.querySelector('#careproIdMenu').style.display="none"
      }).catch((e)=>{
      })
  })
</script>



